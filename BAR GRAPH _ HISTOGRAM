    {
      "cell_type": "code",
      "source": [
        "# Prepare the data\n",
        "# Drop the first two columns as they are not needed\n",
        "data_cleaned = data.drop(columns=['Population, total', 'SP.POP.TOTL'])\n",
        "\n",
        "# The first column is likely the country names\n",
        "data_cleaned.columns = ['Country Name'] + list(data_cleaned.columns[1:])\n",
        "\n",
        "# Melt the DataFrame to long format for better handling\n",
        "data_melted = pd.melt(data_cleaned, id_vars=['Country Name'], var_name='Year', value_name='Population')\n",
        "\n",
        "# Convert 'Year' to datetime format and handle 'Population' column\n",
        "data_melted['Year'] = pd.to_datetime(data_melted['Year'], format='%Y', errors='coerce')\n",
        "data_melted['Population'] = pd.to_numeric(data_melted['Population'], errors='coerce')\n",
        "\n",
        "# Drop rows with missing values\n",
        "data_melted = data_melted.dropna(subset=['Population'])\n"
      ],
      "metadata": {
        "id": "FoQQ4LmRCY_5"
      },
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "plt.figure(figsize=(14, 8))\n",
        "sns.set(style=\"whitegrid\")\n",
        "yearly_population = data_melted.groupby('Year')['Population'].mean()\n",
        "sns.barplot(x=yearly_population.index.year, y=yearly_population.values, palette='viridis')\n",
        "\n",
        "# Add title and labels\n",
        "plt.title('Average Population by Year', fontsize=20)\n",
        "plt.xlabel('Year', fontsize=16)\n",
        "plt.ylabel('Average Population', fontsize=16)\n",
        "\n",
        "# Rotate x labels for better readability\n",
        "plt.xticks(rotation=45, fontsize=12)\n",
        "plt.yticks(fontsize=12)\n",
        "\n",
        "# Show the bar chart\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
