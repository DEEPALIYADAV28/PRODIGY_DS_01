        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import seaborn as sns\n",
        "import zipfile\n",
        "import os\n",
        "\n",
        "# Path to the ZIP file\n",
        "zip_file_path = '/content/drive/MyDrive/P_Data_Extract_From_World_Development_Indicators.zip'\n",
        "\n",
        "# Create a directory to extract the ZIP file contents\n",
        "extract_dir = '/content/drive/MyDrive/P_Data_Extract/'\n",
        "os.makedirs(extract_dir, exist_ok=True)\n",
        "\n",
        "# Extract the ZIP file\n",
        "with zipfile.ZipFile(zip_file_path, 'r') as zip_ref:\n",
        "    zip_ref.extractall(extract_dir)\n",
        "    # List the files in the extracted directory\n",
        "    extracted_files = os.listdir(extract_dir)\n",
        "    print(\"Extracted files:\", extracted_files)\n",
        "\n",
        "# Identify the correct CSV file (usually contains the main data)\n",
        "csv_file_path = os.path.join(extract_dir, '233a35de-90fd-4b80-8e4c-b25ec40a441e_Data.csv')\n",
        "\n",
        "# Load the dataset\n",
        "data = pd.read_csv(csv_file_path, skiprows=4)  # Adjust skiprows if necessary\n",
        "\n",
        "# Display the first few rows to understand its structure\n",
