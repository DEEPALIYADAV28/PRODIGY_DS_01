    {
      "cell_type": "code",
      "source": [
        "\n",
        "\n",
        "# Aggregate population by continent\n",
        "continent_population = data_melted.groupby('Country Name')['Population'].sum()\n",
        "# Assuming you have a way to map countries to continents, add a dummy 'Continent' mapping\n",
        "continent_mapping = {\n",
        "\n",
        "    'United States': 'North America',\n",
        "    'China': 'Asia',\n",
        "    'India': 'Asia',\n",
        "\n",
        "}\n",
        "data_melted['Continent'] = data_melted['Country Name'].map(continent_mapping)\n",
        "continent_population = data_melted.groupby('Continent')['Population'].sum().sort_values(ascending=False)\n",
        "\n",
        "plt.figure(figsize=(14, 8))\n",
        "sns.set(style=\"whitegrid\")\n",
        "sns.barplot(x=continent_population.index, y=continent_population.values, palette='viridis')\n",
        "\n",
        "# Add title and labels\n",
        "plt.title('Total Population by Continent', fontsize=20)\n",
        "plt.xlabel('Continent', fontsize=16)\n",
        "plt.ylabel('Total Population', fontsize=16)\n",
        "\n",
        "# Rotate x labels for better readability\n",
        "plt.xticks(rotation=45, fontsize=12)\n",
        "plt.yticks(fontsize=12)\n",
        "\n",
        "# Show the bar chart\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
