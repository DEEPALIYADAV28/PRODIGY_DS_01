    {
      "cell_type": "code",
      "source": [
        "# Calculate yearly population change\n",
        "data_melted_sorted = data_melted.sort_values(by=['Country Name', 'Year'])\n",
        "data_melted_sorted['Population Change'] = data_melted_sorted.groupby('Country Name')['Population'].diff()\n",
        "\n",
        "# Calculate growth rates (percentage)\n",
        "data_melted_sorted['Growth Rate (%)'] = (data_melted_sorted['Population Change'] / data_melted_sorted['Population'].shift(1)) * 100\n",
        "\n",
        "# Drop rows with missing values for growth rates\n",
        "data_melted_sorted = data_melted_sorted.dropna(subset=['Growth Rate (%)'])\n",
        "\n",
        "plt.figure(figsize=(14, 8))\n",
        "sns.set(style=\"darkgrid\")\n",
        "sns.histplot(data_melted_sorted['Growth Rate (%)'], bins=30, kde=True, color='orange')\n",
        "\n",
        "# Add title and labels\n",
        "plt.title('Distribution of Population Growth Rates', fontsize=20)\n",
        "plt.xlabel('Growth Rate (%)', fontsize=16)\n",
        "plt.ylabel('Frequency', fontsize=16)\n",
        "\n",
        "# Show the histogram\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
